{
  "compilerOptions": {
    "composite": true,
    "module": "CommonJS",
    "target": "ES6",
    "outDir": "./dist/server",
    "rootDir": "./server",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": ["server/**/*.ts"],
  "exclude": ["node_modules"]
}
