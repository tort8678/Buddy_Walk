    "axios": "^1.7.3",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fetch": "^1.1.0",
    "firebase": "^10.12.5",
    "gtfs-realtime-bindings": "^1.1.1",
    "mongodb": "^6.10.0",
    "mongoose": "^8.6.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.4",
    "nunjucks": "^3.2.4",
    "openai": "^4.53.2",
    "react": "^18.3.1",
    "react-camera-pro": "^1.4.0",
    "react-dom": "^18.3.1",
    "react-geolocated": "^4.1.2",
    "react-icons": "^5.5.0",
